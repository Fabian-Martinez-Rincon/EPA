programa prueba
areas 
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    vacias:numero
  comenzar
    derecha
    vacias:=0
    repetir 4
      si ~HayFlorEnLaEsquina & ~HayPapelEnLaEsquina
        vacias:=vacias+1
      mover
      repetir 3
        derecha
    Informar(vacias) 
  fin
variables
  robin:robot1
comenzar
  AsignarArea(robin,ciudad)
  Iniciar(robin,1,1)
fin