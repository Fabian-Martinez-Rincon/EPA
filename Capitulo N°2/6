programa prueba
areas 
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    Pos(75,45)
    repetir 2
      derecha
    mientras (PosCa>15)
      si HayFlorEnLaEsquina
        tomarFlor
        mover
      sino
        mover
  fin
variables
  robin:robot1
comenzar
  AsignarArea(robin,ciudad)
  Iniciar(robin,1,1)
fin