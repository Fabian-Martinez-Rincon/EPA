programa prueba
procesos
  proceso cuadrado(E lado:numero)
  comenzar
    repetir 4
      repetir lado
        mover
      derecha
  fin
areas 
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  comenzar
    cuadrado(5)
    Pos(2,6)
    cuadrado(3)
    Pos(3,9)
    cuadrado(1)
  fin
variables
  robin:robot1
comenzar
  AsignarArea(robin,ciudad)
  Iniciar(robin,1,1)
fin