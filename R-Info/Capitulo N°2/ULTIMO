programa prueba
procesos
  proceso izquierda
  comenzar
    repetir 3
      derecha
  fin
  proceso cuadrado
  comenzar
    repetir 4
      repetir 2
        mover
      derecha
  fin
  proceso torre
  comenzar
    repetir 5
      cuadrado
      Pos(PosAv,PosCa+3)
  fin
areas 
  ciudad: AreaC(1,1,100,100)
robots
  robot robot1
  variables
    vacias:numero
  comenzar
    Pos(2,3)
    repetir 5
      torre
      Pos(PosAv+7,3)
  fin
variables
  robin:robot1
comenzar
  AsignarArea(robin,ciudad)
  Iniciar(robin,1,1)
fin